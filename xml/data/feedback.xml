<?xml version="1.0" encoding="utf-8"?>
<section>
	<modules>
		
		<module id="m2" name="ymaps" title="tttt"/>
		<module id="m1" name="feedback" title="feedback">
			<form title="Форма обратной связи" id="feedback_form" method="post" autocheck="autocheck" baseURI="mysql://%CONNECT%/%TABLE%#" dbConnect="default" dbTable="articles" emailTpl="email_feedback">
				<param name="action" value="feedback"/>
				<param name="section" uri="section"/>
				<param name="module" uri="module"/>
				<param name="sort" uri="sort"/>
				<field type="text" name="name" label="Контактное лицо" check="empty" mail="name" size="40" uri="title"/>
				<field type="textarea" name="message" label="Текст сообщения" rows="6" size="40" mail="message" uri="announce"/>
				<field type="captcha" name="captcha" label="Введите результат выражения с картинки (цифрами)" check="empty" show="1"/>
				<buttongroup>
					<button type="submit" value="Отправить"/>
					<button type="reset" value="Очистить"/>
				</buttongroup>
				<good>&lt;p&gt;asdasd&lt;/p&gt;</good>
				<fail></fail>
			</form>
		</module>
	</modules>
	<template id="default">
		<template id="form"/></template>
</section>
